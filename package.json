  },
  "homepage": "https://github.com/percebus/MSLearn-Azure-DevOps-my-first-extension#readme",
  "devDependencies": {
    "prettier": "^2.7.1",
    "tfx-cli": "^0.12.0",
    "vss-web-extension-sdk": "^5.141.0"
  }
