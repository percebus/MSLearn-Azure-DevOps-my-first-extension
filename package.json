{
  "name": "percebus-first-extension",
  "version": "1.0.1",
  "description": "Azure DevOps my-first-extension",
  "main": "index.js",
  "scripts": {
    "npm:install:global": "npm install --location=global yarn",
    "setup": "npm run npm:install:global",
    "prettier": "prettier .",
    "prettier:write": "npm run prettier -- --write",
    "prettier:check": "npm run prettier -- --check",
    "tfx:extension:create": "npx tfx-cli extension create --rev-version",
    "style": "npm run prettier:write",
    "lint": "npm run prettier:check",
    "test": "exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/percebus/MSLearn-Azure-DevOps-my-first-extension.git"
  },
